<#include "../public_header.ftl">

	<link rel="stylesheet" type="text/css" href="/static/css/codcargo/open.css">
	<script type="text/javascript" src="/static/js/codcargo/open.js"></script>

</head>
<body>

	<div class="zq-body">
        <input type="hidden" value="${goodsId}" name="goodsId">
		<div class="zq-main">
			<div class="title"><@spring.message code="normalGoods_Logisticsprocess"/></div>
			<hr />
			<div class="content">
				<ul class="layui-timeline">
					<span class="no-process layui-text"><@spring.message code="normalGoods_Thegoodshavenotarrivedinthewarehouse"/></span>
				</ul>
			</div>
			<hr/>

			<div class="title">货物信息</div>
			<hr />
			<div class="content goods-info">
				<div class="layui-row">
					<div class="layui-col-md3 tripartiteNumber">
						<label>货物单号：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>

					<div class="layui-col-md3 shipper">
						<label>承运方：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>

					<div class="layui-col-md3 exitWay">
						<label>出境物流方式：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>
                    <div class="layui-col-md3 itemValue">
                        <label>货值：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>

				</div>



				<div class="layui-row">


					<div class="layui-col-md3 tripartiteWeight">
						<label>重量：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>

					<div class="layui-col-md3 totalGoods">
						<label>总箱数：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>
					<div class="layui-col-md6 goodType">
						<label>货物类型：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>

				</div>

                <div class="layui-row">
                    <div class="layui-col-md3 receiptContact">
                        <label>收货人：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>
                    <div class="layui-col-md3 zipCode">
                        <label>邮编：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>

                    <div class="layui-col-md6 receiptContactMobile">
                        <label>收货人联系方式：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>





                </div>

                <div class="layui-row">
                    <div class="layui-col-md12 receiptAddress">
                        <label>收货地址：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>
                </div>
				
			</div>

		</div>

	</div>
	<#-- 放大图片 -->
    <div class="fixed">
        <div>
            <img src="" alt="">
        </div>
    </div>
</body>
</html>