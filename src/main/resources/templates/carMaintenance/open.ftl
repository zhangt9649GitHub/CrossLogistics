<#include "../public_header.ftl">
	<script type="text/javascript" src="/static/js/carMaintenance/open.js"></script>

</head>
<body>

	<div class="zq-body">
        <input type="hidden" value="${carMaintenanceId}" name="carMaintenanceId">
		<div class="zq-main">

			<div class="title"><@spring.message code="truckMission_details"/></div>
			<hr />
			<div class="content shipper-info">
				<div class="layui-row">
					<div class="layui-col-md3 carNumber">
						<label>小车编号：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>
					<div class="layui-col-md3 state">
						<label>任务状态：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>
					
					<div class="layui-col-md3 cbmAddtime">
						<label>创建时间：</label>
						<div class="inline">
							<span></span>
						</div>
					</div>
				</div>
                <div class="layui-row">
                    <div class="layui-col-md12 address">
                        <label>地址：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>


                </div>
                <div class="layui-row">
                    <div class="layui-col-md12 describe">
                        <label>描述：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>


                </div>
                <div class="layui-row">
                    <div class="layui-col-md3 cbmServicetime">
                        <label>维修时间：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>

                    <div class="layui-col-md3 cbmFinishtime">
                        <label>完成时间：</label>
                        <div class="inline">
                            <span></span>
                        </div>
                    </div>
                </div>
				
			</div>

		</div>

	</div>

</body>
</html>