package com.siruiman.crosslogistics;

import com.siruiman.crosslogistics.util.ContextUtils;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.Import;
import org.springframework.transaction.annotation.EnableTransactionManagement;


@SpringBootApplication
@EnableTransactionManagement
@Import(ContextUtils.class)
public class CrosslogisticsApplication {

    public static void main(String[] args) {
        SpringApplication.run(CrosslogisticsApplication.class, args);
    }


}

