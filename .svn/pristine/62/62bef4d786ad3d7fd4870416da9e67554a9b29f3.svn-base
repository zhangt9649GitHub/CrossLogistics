

* {
    margin: 0px;
    padding: 0px;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 12px;
}
/*header start*/
.container {
    width: 100%;
    height: 45px;
    background-color: rgb(8, 135, 215);
    border-bottom: 1px solid rgb(0, 112, 183);
}

.container .logo a {
    float: left;
    color: rgb(255, 255, 255);
    font-size: 18px;
    padding-left: 20px;
    line-height: 45px;
    width: 200px;
    background-color: rgb(0, 112, 183);
}

/* 打开左边导航 */
.container .left-open {
    height: 45px;
    float: left;
}


.container .left-open i {
    display: block;
    color: rgb(255, 255, 255);
    width: 32px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    margin-top: 7px;
    cursor: pointer;
    background: 0px 0px no-repeat rgba(255, 255, 255, 0.1);
    border-radius: 3px;
}
.container .left-open i:hover {
    background: rgba(255,255,255,0.3) url() 0 0 no-repeat;
}
.container .right{
	background-color: rgba(0, 0, 0, 0);
	float: right;
}
.container .layui-nav-item {
    line-height: 45px;
}

.right .layui-nav-item .layui-nav-child{
    right: -10px;
    left:unset ;
}
/* header end */

/* 左边导航栏 start */
.layui-side {
    width: 220px;
    top: 0;
    z-index: 1001;
    top: 46px;
}
.layui-side-menu {
    background-color: #393D49;
    color: #fff;
    box-shadow: 1px 0 2px 0 rgba(0,0,0,.05);
}

.layui-side-menu .layui-side-scroll {
    width: 240px;
}
.layui-side-menu .layui-nav {
    width: 220px;
    margin-bottom: 50px;
    /*background: 0 0;*/
}

