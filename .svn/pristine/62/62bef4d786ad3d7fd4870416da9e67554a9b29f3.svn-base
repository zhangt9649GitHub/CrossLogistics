/* 主体内容 start */

.page-content {
    position: absolute;
    top: 46px;
    right: 0;
    bottom: 0px;
    left: 221px;
    overflow: hidden;
    z-index: 1;

}
.page-content .tab {
    height: 100%;
    width: 100%;
    background: #EFEEF0 0 0 no-repeat;
    margin: 0px;
    position: relative;
}
.page-content .tab .layui-tab-title{
    left: 40px;
    margin-right: 40px;
    background: #fff;
}

.page-content .tab .layui-tab-title .layui-this{
    background: #f6f6f6!important;
}

.page-content .operating{
    position: absolute;
    width: 40px;
    top:0px;
    height: 40px;
    text-align: center;
    cursor: pointer;
    line-height: 40px;
    font-size: 16px;
    border-right: 1px solid #f6f6f6;
    box-sizing: border-box;
}
.page-content .operating .layui-nav-more{
    display: none;
}
.page-content .operating .layui-nav-item .layui-nav-child{

    top: 45px;
}
.page-content .operating .layui-nav .layui-nav-item{
   height: 40px;
}
.page-content .operating .layui-nav{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    padding: 0;
    background: #fff;
}
.page-content .layui-tab-content {
    /*position: absolute;*/
    /*top: 42px;*/
    /*bottom: 0px;*/
    height:100%;
    width: 100%;
    background: #fff 0 0 no-repeat;
    padding: 0px;
    overflow: hidden;
}
.page-content .layui-tab-content .layui-tab-item {
    width: 100%;
    height: 100%;
}
.page-content .layui-tab-content .layui-tab-item iframe {
    width: 100%;
    height: 100%;
}
/* 主体内容 end */