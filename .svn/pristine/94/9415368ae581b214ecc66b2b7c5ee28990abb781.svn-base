package com.siruiman.crosslogistics.model;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(description="郭阳")
public class TruckContactAddress {
    @ApiModelProperty(value = "用户id", example = "1")
    private Integer appUserId;
    @ApiModelProperty(value = "真实姓名", example = "用户姓名")
    private String actualName;
    @ApiModelProperty(value = "手机号", example = "手机号")
    private String mobile;
    @ApiModelProperty(value = "邮编", example = "邮编")
    private String zipCode;
    @ApiModelProperty(value = "地址", example = "地址")
    private String address;
    @ApiModelProperty(value = "门牌号", example = "门牌号")
    private String houseNumber;
    @ApiModelProperty(value = "房屋类型 1普通 2低屋", example = "1")
    private Integer addressType;

    public Integer getAppUserId() {
        return appUserId;
    }

    public void setAppUserId(Integer appUserId) {
        this.appUserId = appUserId;
    }

    public String getActualName() {
        return actualName;
    }

    public void setActualName(String actualName) {
        this.actualName = actualName;
    }

    public String getMobile() {
        return mobile;
    }

    public void setMobile(String mobile) {
        this.mobile = mobile;
    }

    public String getZipCode() {
        return zipCode;
    }

    public void setZipCode(String zipCode) {
        this.zipCode = zipCode;
    }

    public String getAddress() {
        return address;
    }

    public void setAddress(String address) {
        this.address = address;
    }

    public String getHouseNumber() {
        return houseNumber;
    }

    public void setHouseNumber(String houseNumber) {
        this.houseNumber = houseNumber;
    }

    public Integer getAddressType() {
        return addressType;
    }

    public void setAddressType(Integer addressType) {
        this.addressType = addressType;
    }

    @Override
    public String toString() {
        return "TruckContactAddress{" +
                "appUserId=" + appUserId +
                ", actualName='" + actualName + '\'' +
                ", mobile='" + mobile + '\'' +
                ", zipCode='" + zipCode + '\'' +
                ", address='" + address + '\'' +
                ", houseNumber='" + houseNumber + '\'' +
                ", addressType=" + addressType +
                '}';
    }
}
