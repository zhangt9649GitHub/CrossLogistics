

/* 图片 start */
.images img{
    margin: 0 5px 5px 0;
    max-height: 300px;
    width:calc(33.3% - 10px);
}
.images img:nth-child(3n){
    margin-right: 0px;
}

/* 图片 end */

/* 查看原图 start */

.fixed{
    position: fixed;
    top:0;
    bottom:0;
    left:0;
    right:0;
    background-color:rgba(0,0,0,0.3);
    display: none;
}
.fixed div{
    position: relative;
    width: 100%;
    height: 100%;
}
.fixed img{
    margin: auto;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}
/* 查看原图 end */