package com.siruiman.crosslogistics.model;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(description="郭阳")
public class AppTaskOrderGoods {
    @ApiModelProperty(value = "货物id", example = "0")
    private Integer goodsId;
    @ApiModelProperty(value = "邮编", example = "zipCode")
    private String zipCode;
    @ApiModelProperty(value = "收货地址", example = "receiptAddress")
    private String receiptAddress;

    public Integer getGoodsId() {
        return goodsId;
    }

    public void setGoodsId(Integer goodsId) {
        this.goodsId = goodsId;
    }

    public String getZipCode() {
        return zipCode;
    }

    public void setZipCode(String zipCode) {
        this.zipCode = zipCode;
    }

    public String getReceiptAddress() {
        return receiptAddress;
    }

    public void setReceiptAddress(String receiptAddress) {
        this.receiptAddress = receiptAddress;
    }

    @Override
    public String toString() {
        return "AppTaskOrderGoods{" +
                "goodsId=" + goodsId +
                ", zipCode='" + zipCode + '\'' +
                ", receiptAddress='" + receiptAddress + '\'' +
                '}';
    }
}
