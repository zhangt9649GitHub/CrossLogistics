        FROM goods AS gd
        LEFT JOIN goods_details gds ON (gd.goods_id=gds.goods_id)
        LEFT JOIN (select goods_id,operate_type,logistic_info_id from logistic_info as a,(select max(logistic_info_id) AS C from logistic_info group by goods_id ) AS b  WHERE a.goods_id>0 AND a.logistic_info_id = b.C ) AS li
        ON (li.goods_id = gd.goods_id)
        <where>
            <if test="tripartiteNumber != null and tripartiteNumber != ''">
                AND tripartite_number like "%"#{tripartiteNumber}"%"
            </if>
            <if test="deliveryNumber != null and deliveryNumber != ''">
                AND delivery_number like "%"#{deliveryNumber}"%"
            </if>
            <if test="seachTime != null and seachTime != ''">
                AND gd.add_time like "%"#{seachTime}"%"
            </if>
            <if test="operateResult != null and operateResult != ''">
                AND li.operate_type like "%"#{operateResult}"%"
            </if>
            <if test="warehouseId != null and warehouseId != ''">
                AND gd.warehouse_id = #{warehouseId}
            </if>
