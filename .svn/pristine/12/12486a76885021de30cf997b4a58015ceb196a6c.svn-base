package com.siruiman.crosslogistics.model;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

import java.math.BigDecimal;
@ApiModel(description = "郝腾")
public class GoodsOrderStatistical {

    @ApiModelProperty(value = "今日订单数", example = "1")
    private int dailyOrderQuantity;

    @ApiModelProperty(value = "今日派送成功订单数", example = "1")
    private int toSendSuccessfully;

    @ApiModelProperty(value = "今日派送件货到付款总金额数", example = "1")
    private BigDecimal totalAmountCollectOnDelivery;

    @ApiModelProperty(value = "转运订单人民币总收款（单位 人民币）", example = "1")
    private BigDecimal transshipmentRmbCollection;

    @ApiModelProperty(value = "转运订单美分总收款（单位 美分）", example = "1")
    private BigDecimal transshipmentCentCollection;

    @ApiModelProperty(value = "转运订单新币总收款（单位 新分）", example = "1")
    private BigDecimal transshipmentSGDCentCollection;

    @ApiModelProperty(value = "余额提现总支出（单位 人民币）", example = "1")
    private BigDecimal withdrawalSpending;

    @ApiModelProperty(value = "余额提现总支出（单位 新币）", example = "1")
    private BigDecimal withdrawalSGDSpending;


    public int getDailyOrderQuantity() {
        return dailyOrderQuantity;
    }

    public void setDailyOrderQuantity(int dailyOrderQuantity) {
        this.dailyOrderQuantity = dailyOrderQuantity;
    }

    public int getToSendSuccessfully() {
        return toSendSuccessfully;
    }

    public void setToSendSuccessfully(int toSendSuccessfully) {
        this.toSendSuccessfully = toSendSuccessfully;
    }

    public BigDecimal getTotalAmountCollectOnDelivery() {
        return totalAmountCollectOnDelivery;
    }

    public void setTotalAmountCollectOnDelivery(BigDecimal totalAmountCollectOnDelivery) {
        this.totalAmountCollectOnDelivery = totalAmountCollectOnDelivery;
    }

    public BigDecimal getTransshipmentRmbCollection() {
        return transshipmentRmbCollection;
    }

    public void setTransshipmentRmbCollection(BigDecimal transshipmentRmbCollection) {
        this.transshipmentRmbCollection = transshipmentRmbCollection;
    }

    public BigDecimal getTransshipmentCentCollection() {
        return transshipmentCentCollection;
    }

    public void setTransshipmentCentCollection(BigDecimal transshipmentCentCollection) {
        this.transshipmentCentCollection = transshipmentCentCollection;
    }

    public BigDecimal getWithdrawalSpending() {
        return withdrawalSpending;
    }

    public void setWithdrawalSpending(BigDecimal withdrawalSpending) {
        this.withdrawalSpending = withdrawalSpending;
    }

    public BigDecimal getTransshipmentSGDCentCollection() {
        return transshipmentSGDCentCollection;
    }

    public void setTransshipmentSGDCentCollection(BigDecimal transshipmentSGDCentCollection) {
        this.transshipmentSGDCentCollection = transshipmentSGDCentCollection;
    }

    public BigDecimal getWithdrawalSGDSpending() {
        return withdrawalSGDSpending;
    }

    public void setWithdrawalSGDSpending(BigDecimal withdrawalSGDSpending) {
        this.withdrawalSGDSpending = withdrawalSGDSpending;
    }
}
