/* 查看原图 start */

.fixed{
    position: fixed;
    top:0;
    bottom:0;
    left:0;
    right:0;
    background-color:rgba(0,0,0,0.3);
    display: none;
}
.fixed div{
    position: relative;
    width: 100%;
    height: 100%;
}
.fixed img{
    margin: auto;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}
/* 查看原图 end */
