<#include "../public_header.ftl">


	<script type="text/javascript" src="/static/js/generalcargo/add.js"></script>

</head>
<body>

	<div class="zq-body">
		<form class="layui-form zq-form">
			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					货物单号
				</div>
				<div class="layui-col-md8">
					<input type="text" name="tripartiteNumber" class="layui-input" autocomplete="off" lay-verify="required|number" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					承运方
				</div>
				<div class="layui-col-md8">
					<input type="text" name="shipper" class="layui-input" autocomplete="off" lay-verify="required"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					收货人
				</div>
				<div class="layui-col-md8">
					<input type="text" name="receiptContact" class="layui-input" autocomplete="off" lay-verify="required"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					收货人联系方式
				</div>
				<div class="layui-col-md8">
					<input type="text" name="receiptContactMobile" class="layui-input" autocomplete="off" lay-verify="required"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					收货地址
				</div>
				<div class="layui-col-md8">
					<input type="text" name="receiptAddress" class="layui-input" autocomplete="off" lay-verify="required"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					邮编
				</div>
				<div class="layui-col-md8">
					<input type="text" name="zipCode" class="layui-input" autocomplete="off" lay-verify="required|number"/>
				</div>
			</div>


			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					货物类型
				</div>
				<div class="layui-col-md8">
					<input type="text" name="goodType" class="layui-input" autocomplete="off" lay-verify="required"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					货物重量（单位：kg）
				</div>
				<div class="layui-col-md8">
					<input type="text" name="tripartiteWeight" class="layui-input" autocomplete="off" lay-verify="required|number"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					总箱数
				</div>
				<div class="layui-col-md8">
					<input type="text" name="totalGoods" class="layui-input" autocomplete="off" lay-verify="required|number"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					货值
				</div>
				<div class="layui-col-md8">
					<input type="text" name="itemValue" class="layui-input" autocomplete="off" lay-verify="required|number"/>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b>*</b>
					出境物流方式
				</div>
				<div class="layui-col-md8">
					<select name="exitWay" lay-verify="required">
						<option value=""></option>
						<option value="海运">海运</option>
						<option value="空运">空运</option>
					</select>
				</div>
			</div>



			<div class="zq-submit">
				<button class="layui-btn layui-btn-normal" lay-submit="" lay-filter="add"> <@spring.message code="zq_save"/></button>
			</div>
		</form>




	</div>

</body>
</html>