<#include "../public_header.ftl">


	<script type="text/javascript" src="/static/js/normalGoods/edit.js"></script>

</head>
<body>

	<div class="zq-body">
		<input type="hidden" value="${from}" name="from">
		<form class="layui-form zq-form">
			<input type="hidden" value="${goodsId}" name="goodsId">
			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					收货人
				</div>
				<div class="layui-col-md8">
					<input type="text" name="receiptContact" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					收货人联系方式
				</div>
				<div class="layui-col-md8">
					<input type="text" name="receiptContactMobile" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					收货地址
				</div>
				<div class="layui-col-md8">
					<input type="text" name="receiptAddress" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					邮编
				</div>
				<div class="layui-col-md8">
					<input type="text" name="zipCode" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom zhuanyun">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					用户编号
				</div>
				<div class="layui-col-md8">
					<input type="text" name="number" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom sanfang">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					发货人
				</div>
				<div class="layui-col-md8">
					<input type="text" name="shipContact" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom sanfang">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					发货人联系方式
				</div>
				<div class="layui-col-md8">
					<input type="text" name="shipContactMobile" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom sanfang">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					发货地址
				</div>
				<div class="layui-col-md8">
					<input type="text" name="shipAddress" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					货物单号
				</div>
				<div class="layui-col-md8">
					<input type="text" name="tripartiteNumber" class="layui-input" autocomplete="off" readonly />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					特殊货物
				</div>
				<div class="layui-col-md8">
					<select name="isSpecialGoods" >
						<option value=""></option>
						<option value="1">是</option>
						<option value="2">否</option>
					</select>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom zhuanyun">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					货物分类
				</div>
				<div class="layui-col-md8">
					<select name="category" >
						<option value=""></option>
					</select>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					货物类型
				</div>
				<div class="layui-col-md8">
					<input type="text" name="goodType" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					长（单位：cm）
				</div>
				<div class="layui-col-md8">
					<input type="text" name="actualLong" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					宽（单位：cm）
				</div>
				<div class="layui-col-md8">
					<input type="text" name="actualWidth" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					高（单位：cm）
				</div>
				<div class="layui-col-md8">
					<input type="text" name="actualHeight" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					重量（单位：kg）
				</div>
				<div class="layui-col-md8">
					<input type="text" name="actualWeight" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom zhuanyun">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					入库仓位
				</div>
				<div class="layui-col-md8">
					<input type="text" name="intoWpNumber" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					出库仓位
				</div>
				<div class="layui-col-md8">
					<input type="text" name="outWpNumber" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					出境物流方式
				</div>
				<div class="layui-col-md8">
					<select name="exitWay" >
						<option value=""></option>
						<option value="海运">海运</option>
						<option value="空运">空运</option>
					</select>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom zhuanyun">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					加急派送
				</div>
				<div class="layui-col-md8">
					<select name="isUrgent" >
						<option value=""></option>
						<option value="是">是</option>
						<option value="否">否</option>
					</select>
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					货值
				</div>
				<div class="layui-col-md8">
					<input type="text" name="itemValue" class="layui-input" autocomplete="off" lay-verify="number" />
				</div>
			</div>

			<div class="layui-col-md6 zq-col-bottom">
				<div class="layui-col-md4 zq-col-left">
					<b></b>
					货袋编号
				</div>
				<div class="layui-col-md8">
					<input type="text" name="bagNumber" class="layui-input" autocomplete="off" />
				</div>
			</div>

			<div class="layui-col-md12 zq-col-bottom">
				<div class="layui-col-md2 zq-col-left">
					<b></b>
					备注
				</div>
				<div class="layui-col-md10">
					<textarea name="remark" class="layui-textarea"></textarea>
				</div>
			</div>


			<div class="zq-submit">
				<button class="layui-btn layui-btn-normal" lay-submit="" lay-filter="add"> <@spring.message code="zq_save"/></button>
			</div>
		</form>




	</div>

</body>
</html>